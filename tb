#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55828be79d30 .scope module, "tb_instruction_memory" "tb_instruction_memory" 2 3;
 .timescale -9 -12;
v0x55828bea00c0_0 .net "instruction", 31 0, L_0x55828bea0370;  1 drivers
v0x55828bea01b0_0 .var "read_address", 31 0;
S_0x55828be88610 .scope module, "dut" "instruction_memory" 2 10, 3 1 0, S_0x55828be79d30;
 .timescale 0 0;
    .port_info 0 /INPUT 32 "read_address";
    .port_info 1 /OUTPUT 32 "instruction";
L_0x55828bea0370 .functor BUFZ 32, L_0x55828bea0280, C4<00000000000000000000000000000000>, C4<00000000000000000000000000000000>, C4<00000000000000000000000000000000>;
v0x55828be50ba0_0 .net *"_ivl_0", 31 0, L_0x55828bea0280;  1 drivers
v0x55828be9fe00_0 .net "instruction", 31 0, L_0x55828bea0370;  alias, 1 drivers
v0x55828be9fee0 .array "instructions", 0 31, 31 0;
v0x55828be9ff80_0 .net "read_address", 31 0, v0x55828bea01b0_0;  1 drivers
L_0x55828bea0280 .array/port v0x55828be9fee0, v0x55828bea01b0_0;
    .scope S_0x55828be88610;
T_0 ;
    %vpi_call 3 9 "$readmemh", "data.hex", v0x55828be9fee0 {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x55828be79d30;
T_1 ;
    %vpi_call 2 17 "$display", "==========================================" {0 0 0};
    %vpi_call 2 18 "$display", "   Instruction Memory Testbench" {0 0 0};
    %vpi_call 2 19 "$display", "==========================================" {0 0 0};
    %vpi_call 2 20 "$display", "Address    | Instruction" {0 0 0};
    %vpi_call 2 21 "$display", "------------------------------------------" {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55828bea01b0_0, 0, 32;
    %delay 10000, 0;
    %vpi_call 2 26 "$display", "0x%08h | 0x%08h", v0x55828bea01b0_0, v0x55828bea00c0_0 {0 0 0};
    %pushi/vec4 1, 0, 32;
    %store/vec4 v0x55828bea01b0_0, 0, 32;
    %delay 10000, 0;
    %vpi_call 2 31 "$display", "0x%08h | 0x%08h", v0x55828bea01b0_0, v0x55828bea00c0_0 {0 0 0};
    %pushi/vec4 2, 0, 32;
    %store/vec4 v0x55828bea01b0_0, 0, 32;
    %delay 10000, 0;
    %vpi_call 2 36 "$display", "0x%08h | 0x%08h", v0x55828bea01b0_0, v0x55828bea00c0_0 {0 0 0};
    %pushi/vec4 3, 0, 32;
    %store/vec4 v0x55828bea01b0_0, 0, 32;
    %delay 10000, 0;
    %vpi_call 2 41 "$display", "0x%08h | 0x%08h", v0x55828bea01b0_0, v0x55828bea00c0_0 {0 0 0};
    %pushi/vec4 31, 0, 32;
    %store/vec4 v0x55828bea01b0_0, 0, 32;
    %delay 10000, 0;
    %vpi_call 2 46 "$display", "0x%08h | 0x%08h", v0x55828bea01b0_0, v0x55828bea00c0_0 {0 0 0};
    %pushi/vec4 32, 0, 32;
    %store/vec4 v0x55828bea01b0_0, 0, 32;
    %delay 10000, 0;
    %vpi_call 2 51 "$display", "0x%08h | 0x%08h (out of bounds)", v0x55828bea01b0_0, v0x55828bea00c0_0 {0 0 0};
    %vpi_call 2 53 "$display", "==========================================" {0 0 0};
    %vpi_call 2 54 "$display", "Testbench Complete" {0 0 0};
    %vpi_call 2 55 "$display", "==========================================" {0 0 0};
    %vpi_call 2 56 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
